{
  "name": "techcollors",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && npm run wait-postgres && npm run migrations:up && next dev",
    "migrations:create": "node-pg-migrate create -m src/infra/migrations",
    "migrations:up": "node-pg-migrate -m src/infra/migrations --envPath .env.development up",
    "services:down": "docker compose -f src/infra/compose.yaml down",
    "services:stop": "docker compose -f src/infra/compose.yaml stop",
    "services:up": "docker compose -f src/infra/compose.yaml up -d",
    "wait-postgres": "node src/infra/scripts/wait-for-postgres.js",
    "test": "npm run services:up  && concurrently -n next,jest --hide next -k -s first \"next dev\" \"jest --silent --runInBand --verbose\"",
    "test:watch": "jest --watchAll --runInBand --verbose ",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:eslint:check": "npm run lint .",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "commit": "cz"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "async-retry": "1.3.3",
    "bcryptjs": "3.0.3",
    "browserslist": "4.28.0",
    "cookie": "1.0.2",
    "dotenv": "17.2.3",
    "dotenv-expand": "12.0.3",
    "jest": "30.2.0",
    "lucide": "0.554.0",
    "lucide-react": "0.554.0",
    "next": "16.0.7",
    "next-connect": "1.0.0",
    "node-pg-migrate": "6.2.2",
    "nodemailer": "7.0.5",
    "pg": "8.16.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "styled-components": "6.1.19",
    "swr": "2.3.7",
    "typescript": "5.9.3",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@faker-js/faker": "9.7.0",
    "babel-plugin-react-compiler": "1.0.0",
    "babel-plugin-styled-components": "2.1.4",
    "baseline-browser-mapping": "2.8.32",
    "commitizen": "4.3.1",
    "concurrently": "8.2.2",
    "cz-conventional-changelog": "3.0.1",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.6",
    "prettier": "3.7.3",
    "set-cookie-parser": "2.7.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
